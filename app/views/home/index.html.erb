<div class="padding50px">
  <%= link_to 'New Upload File', new_upload_files_path, class: 'btn btn-primary' %>
  <%= link_to 'Custom Link File', custom_link_file_path, class: 'btn btn-default' %>
  <table class="table margintop20px margintop20px">
      <% @upload_files.each do |upload_file| %>
          <tr>
              <td><%= link_to "#{upload_file.file_html_file_name}", upload_file.file_html.url %></td>
              <td><%= link_to 'Delete', destroy_upload_files_path(upload_file.id), method: 'delete', data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% end %>
      <tr>
        <td class="center-positon" colspan="2">
            <%= paginate @upload_files %>
        </td>
      </tr>
  </table>
</div>
